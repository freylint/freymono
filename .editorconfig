# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[*.json]
index_style = space
indent_size = 2

[*.css]
index_style = space
indent_size = 2

[*.bzl]
index_style = space
indent_size = 2

[*.nix]
indent_style = space
indent_size = 2
